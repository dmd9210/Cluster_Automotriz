@model IEnumerable<ClusterAutomotriz_Model.Empresa_Actividad>

<table class="table">
    <tr>
        <th>            
            @Html.DisplayNameFor(model => model.nombreEmpresa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombreActividad)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombreEmpresa)
        </td>
        <td>            
            @Html.DisplayFor(modelItem => item.nombreActividad)
        </td>
    </tr>
}

</table>
